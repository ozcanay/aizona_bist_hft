{
    "terminal.integrated.defaultProfile.linux": "bash",
    "terminal.integrated.profiles.linux": {
        "bash": {
            "path": "/usr/bin/bash",
            "args": [
                "-l"
            ]
        }
    },
    "clangd.path": "/usr/bin/clangd",
    "clangd.arguments": [
        "-compile-commands-dir=.vscode",
        "-log=verbose",
        "-pretty",
        "--background-index",
        "--completion-style=detailed",
        "--header-insertion=never",
